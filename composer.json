{
    "name": "friendsofsymfony/elastica-bundle",
    "type": "symfony-bundle",
    "description": "Elasticsearch PHP integration for your Symfony project using Elastica",
    "keywords": [
        "doctrine2",
        "elastica",
        "elasticsearch",
        "mongodb",
        "search"
    ],
    "homepage": "https://github.com/FriendsOfSymfony/FOSElasticaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/FriendsOfSymfony/FOSElasticaBundle/contributors"
        },
        {
            "name": "Tim Nagel",
            "email": "tim@nagel.com.au"
        },
        {
            "name": "Richard Miller",
            "email": "richard.miller@limethinking.co.uk"
        },
        {
            "name": "Jeremy Mikola",
            "email": "jmikola@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/property-access": "^5.4",
        "pagerfanta/pagerfanta": "^2.0",
        "psr/log": "^1.0|^2.0",
        "ruflin/elastica": "^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.9",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/phpcr-bundle": "^2.0",
        "doctrine/phpcr-odm": "^1.4",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "jms/serializer-bundle": "^3.0",
        "phpunit/phpunit": "^9.5",
        "knplabs/knp-components": "^1.2",
        "symfony/expression-language": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/yaml": "^5.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/web-profiler-bundle": "^5.4",
        "doctrine/mongodb-odm": "^2.4",
        "doctrine/mongodb-odm-bundle": "^4.4",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "suggest": {
        "enqueue/elastica-bundle": "The bundle adds extra features to FOSElasticaBundle bundle. Aimed to improve performance."
    },
    "autoload": {
        "psr-4": { "FOS\\ElasticaBundle\\": "src/" },
        "files": ["src/DoctrinePersistenceCompat.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\ElasticaBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}
